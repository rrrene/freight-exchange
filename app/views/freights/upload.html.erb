<%= form_tag({}, :multipart => true) do %>
  <%= file_field_tag :data %>
  <%= submit_tag %>
<% end %>

<% if @sheet %>
  <%= t("postings.upload.created_postings", :count => @collection.size)%>

<pre>
  <% @collection.each_with_index do |resource, i| %>
    index: <%= i %>
    attributes (by upload): <%= @posting_attributes[i].attributes.inspect %>
    attributes (by resource): <%= resource.attributes.inspect %>  
    valid?: <%= resource.valid? %>
    
  <% end %>
</pre>
<% end %>