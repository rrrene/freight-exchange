<% form_for(@freight, :html => {:class => 'formtastic'}) do |f| %>
  <% %w(origin destination).each do |site| %>
    <fieldset class="inputs">
      <legend><%= site %></legend>
      <%= render :partial => 'site_info_form_content', :locals => {:f => f, :site => site} %>
    </fieldset>
  <% end %>
<% end %>